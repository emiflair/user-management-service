NODE_ENV=production
PORT=5000

# MongoDB connection - Docker Compose
MONGO_URI=mongodb://mongo:27017/user-service

# JWT
JWT_SECRET=change_this_to_a_long_random_32+_char_secret
JWT_EXPIRES_IN=1h

# CORS & security limits
CORS_ORIGIN=*
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=300
BCRYPT_SALT_ROUNDS=12
